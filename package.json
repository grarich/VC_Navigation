{
  "name": "vc_navigation",
  "version": "0.0.1",
  "description": "DiscordのVCに常駐するBot",
  "main": "src/index.ts",
  "scripts": {
    "launch": "export $(cat .env | grep -v ^# | xargs); tsc -p . && node ./.build/index.js",
    "dev": "export $(cat .env | grep -v ^# | xargs); ts-node-dev ./src/index.ts",
    "build": "tsc -p .",
    "build:test": "tsc --noEmit"
  },
  "author": "grarich",
  "dependencies": {
    "@discordjs/voice": "^0.8.0",
    "@sapphire/framework": "next",
    "discord.js": "^13.6.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "ts-node": "^10.6.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "repository": "https://github.com/grarich/VC_Navigation",
  "license": "MIT"
}
